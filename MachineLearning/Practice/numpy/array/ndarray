import numpy as np

list = [1, 2, 3] 
print(type(list))
array1 = np.array(list)
print(type(array1))
print(array1.shape)

array2 = np.array([[1, 2, 3], [2, 3, 4]])
print(type(array2))
print(array2.shape)

array3 = np.array([[1, 2, 3]])
print(type(array3))
print(array3.shape)

print(array1.ndim)
print(array2.ndim)
print(array3.ndim)
